<div class="container-fluid">

    <div class="card-body">
        <div class="table-responsive">
            <div class="header_table">
                <% if (data[0].role=='customer' ) { %>
                    <p>Data Customer</p>

                    <% }else if (data[0].role=='tukang' ){ %>
                        <p>Data Tukang</p>
                        <% } %>
            </div class="table_field">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead class="data-table">
                    <tr>
                        <th>No</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Address</th>
                        <th>Phone</th>
                        <% if (data[0].role=='customer' ) { %>
                            <th>Role</th>

                            <% }else if (data[0].role=='tukang' ){ %>
                                <th>Service</th>
                                <% } %>
                                    <th>Delete</th>
                    </tr>
                </thead>
                <tbody>
                    <% let no=1; data.forEach(element=> { %>

                        <tr>
                            <th scope="row">
                                <%= no %>
                            </th>
                            <td>
                                <%= element.name %>
                            </td>
                            <td>
                                <%= element.email %>
                            </td>
                            <td>
                                <%= element.address %>
                            </td>
                            <td>
                                <%= element.phone %>
                            </td>
                            <% if (element.role=='customer' ) { %>
                                <td>
                                    <%= element.role %>
                                </td>

                                <% }else if (element.role=='tukang' ){ %>
                                    <td>
                                        <%= element.service %>
                                    </td>
                                    <% } %>
                                        <td>
                                            <a type="button" href="/akun/delete/<%= element.id %>"
                                                class="btn btn-danger">Delete</a>
                                        </td>
                        </tr>

                        <% no++ }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>